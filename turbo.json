{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    "tsconfig.json"
  ],
  "pipeline": {
    "build": {
      "outputs": [
        "!.next/cache/**",
        ".next/**",
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "//#integrity:repair": {
      "cache": false
    },
    "integrity:repair": {
      "cache": false
    },
    "//#integrity:validate": {
      "outputs": []
    },
    "integrity:validate": {
      "outputs": []
    },
    "start": {
      "cache": false,
      "dependsOn": [
        "build"
      ],
      "persistent": true
    }
  }
}
