{
  "name": "ui-mdx",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin typescript",
    "integrity:repair": "pnpm run integrity:validate --fix",
    "integrity:validate": "eslint . --ext cjs,js,json,mjs,ts,tsx",
    "start:dev": "pnpm run build --watch"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^3.0.0",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/mdx": "^2.0.10",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "eslint-config-custom": "workspace:*",
    "react": "^18.2.0",
    "rollup": "^4.9.4",
    "tsconfig": "workspace:*"
  },
  "eslintConfig": {
    "extends": "custom",
    "root": true
  }
}
