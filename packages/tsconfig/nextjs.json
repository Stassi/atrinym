{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "jsx": "preserve",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2023"
    ],
    "noEmit": true,
    "resolveJsonModule": true,
    "strict": false,
    "strictNullChecks": true
  },
  "display": "Next.js",
  "extends": "./base.json"
}
