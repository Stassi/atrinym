{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "noEmit": true,
    "resolveJsonModule": true,
    "strict": false,
    "strictNullChecks": true
  },
  "display": "Next.js",
  "extends": "./base.json"
}
