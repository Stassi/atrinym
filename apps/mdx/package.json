    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ui": "workspace:*"
  },
  "devDependencies": {
    "@types/mdx": "^2.0.10",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*"
  },
  "eslintConfig": {
    "extends": "custom",
    "root": true
  }
